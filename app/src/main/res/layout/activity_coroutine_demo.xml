<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kotlin.KotlinMainActivity">

    <Button
        android:id="@+id/btn_run_blocking"
        style="@style/TextStyle_White_Normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:background="@drawable/radius_10_no_stroke_black66_bg_shape"
        android:padding="@dimen/dimens_mid"
        android:text="runBlocking"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/btn_launch"
        style="@style/TextStyle_White_Normal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:background="@drawable/radius_10_no_stroke_black66_bg_shape"
        android:padding="@dimen/dimens_mid"
        android:text="launch"
        app:layout_constraintStart_toEndOf="@+id/btn_run_blocking"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>